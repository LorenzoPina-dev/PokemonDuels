<UserControl x:Class="pokemonDuel.Battaglia"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:pokemonDuel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" SizeChanged="UserControl_SizeChanged">
    <Grid>
        <Grid x:Name="Attacco">
            <WindowsFormsHost x:Name="host" Background="Transparent"/>
            <Canvas x:Name="CanvasAttacco" ></Canvas>
        </Grid>
        <Canvas Name="myCanvas"/>
    </Grid>
</UserControl>
