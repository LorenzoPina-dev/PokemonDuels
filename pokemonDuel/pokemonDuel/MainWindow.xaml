<Window x:Class="pokemonDuel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pokemonDuel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" SizeChanged="Window_SizeChanged">
    <DockPanel>
        <WindowsFormsHost DockPanel.Dock="Right" x:Name="host" Width="300" Height="400"/>
        <Canvas Name="myCanvas" DockPanel.Dock="Left" Height="400" Width="440"></Canvas>
    </DockPanel>
</Window>
